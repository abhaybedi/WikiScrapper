{
    "manifest_version": 2,
    "name": "ScrapeMaster",
    "version": "1.0",
    "description": "I scrape.",
    "permissions": [
      "activeTab",
      "http://*/*",
      "https://*/*"
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "browser_action": {
      "default_popup": "popup.html"
    },
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
    "web_accessible_resources": ["popup.html"]
  }
  